<SpinnerOption>:



<Informe>:
    layout_content: layout_content

    BoxLayout:
        orientation: 'vertical'
        Label:
            text: root.name
        ScrollView:
            id: scroll_view
            size: self.size
            GridLayout:
                id: layout_content
                size_hint_y: None
                cols: 2
                row_default_height: '20dp'
                row_force_default: True
                spacing: 0, 0
                padding: 0, 0
        BoxLayout:
            orientation: 'horizontal'
            Button:
                text: 'Volver a generador'
                on_press: root.btn_volver()
